<h1>New Group</h1>

    <%= render partial: "layouts/errors", locals: {object: @group} %>

<%= form_for(@group) do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <br>

    <%= f.label :description %>
    <%= f.text_field :description %>
    <br>
    <br>

    <%= f.label :rules %>
    <%= f.text_field :rules %>
    <br>
    <br>

    <%= f.submit %>
<% end %>